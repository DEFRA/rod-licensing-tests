{
  "name": "frontend-acceptance-tests",
  "version": "0.0.0",
  "description": "Acceptance tests for the Get a Fishing Licence frontend",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "rod",
    "fishing",
    "licence",
    "service",
    "qa",
    "tests"
  ],
  "author": "DEFRA",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git://github.com/DEFRA/rod-licensing-tests.git",
    "directory": "packages/frontend-acceptance-tests"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "report:generate": "allure generate allure-results --clean -o allure-report",
    "report:open": "allure open allure-report",
    "axe": "./node_modules/.bin/wdio ./conf/local.conf.mjs --spec ./features/axe.feature",
    "local": "node --experimental-require-module ./node_modules/.bin/wdio ./conf/local.conf.mjs"
  },
  "dependencies": {
    "@wdio/allure-reporter": "9.17.0",
    "@wdio/cli": "8.32.3",
    "@wdio/cucumber-framework": "8.32.3",
    "@wdio/junit-reporter": "8.32.2",
    "@wdio/local-runner": "8.32.3",
    "@wdio/spec-reporter": "8.32.2",
    "allure-commandline": "2.34.1",
    "axe-core": "4.8.1",
    "chai": "4.3.8",
    "defra-logging-facade": "git+https://github.com/DEFRA/defra-logging-facade.git#b11d97e",
    "dynamics-web-api": "1.7.3",
    "json-to-csv": "1.0.0",
    "moment": "2.29.0",
    "simple-oauth2": "4.1.0",
    "webdriverio": "8.32.3"
  }
}
